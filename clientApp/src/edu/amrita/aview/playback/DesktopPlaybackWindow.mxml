<s:Window xmlns:fx="http://ns.adobe.com/mxml/2009" xmlns:s="library://ns.adobe.com/flex/spark" xmlns:mx="library://ns.adobe.com/flex/mx" creationComplete="init(event)" title="A-VIEW Desktop Playback(A-VIEW Classroom)">
	<fx:Script>
		<![CDATA[
			import mx.events.FlexEvent;
			/**
			 * Refers for DesktopPlayback class
			 */			
			public var desktopPlaybackComp:DesktopPlayback=new DesktopPlayback();	
			
			/**
			 * For adding the DesktopPlayback in to seperate window
			 *
			 * @param event of FlexEvent
			 * @return void
			 */
			protected function init(event:FlexEvent):void{
				this.addElement(desktopPlaybackComp);
			}
		]]>
	</fx:Script>
</s:Window>
