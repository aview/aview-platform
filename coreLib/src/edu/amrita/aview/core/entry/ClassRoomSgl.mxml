		 xmlns:s="library://ns.adobe.com/flex/spark"
		 xmlns:mx="library://ns.adobe.com/flex/mx"
		 xmlns:local="*"
		 xmlns:MultipleViewerInteraction="edu.amrita.aview.core.video.*"
		 xmlns:components="edu.amrita.aview.core.shared.components.*"
		 top="0" bottom="0" width="100%" creationPolicy="all" horizontalGap="0" paddingLeft="0">
	<fx:Script>
		<![CDATA[
			import edu.amrita.aview.core.login.boilerplate.Strings;
			
			import mx.controls.Alert;
		]]>
	</fx:Script>
	<fx:Script source="ClassRoomSglHandler.as"/>
	<fx:Declarations>
		<s:Resize id="controlvidboxHide" duration="1000"
				  effectEnd="controlvidboxHide_effectEndHandler(event)" target="{controlvidbox}"
				  widthFrom="{controlvidbox.width}" widthTo="0"/>
		<mx:Dissolve id="disolveEffect"/>
	</fx:Declarations>
	
	
	<mx:VBox id="controlvidbox" x="0" y="0" width="21%" height="100%" minWidth="250" maxWidth="250"
			 borderVisible="false"
			 creationComplete="{FlexGlobals.topLevelApplication.mainApp.mainContainerComp.classroomComp.btnResize.left=controlvidbox.width-10}"
			 creationPolicy="all" horizontalAlign="center" horizontalScrollPolicy="off"
			 paddingLeft="0" resize="controlvidbox_resizeHandler(event)" verticalGap="0"
			 verticalScrollPolicy="off">
		<MultipleViewerInteraction:VideoStreamDisplay id="pnlTeacher" visible="false"
													   
													   includeInLayout="false" 
													   showCloseButton="true" title="Presenter"/>
		<s:BorderContainer width="100%" height="39" borderVisible="false">
			<s:Rect width="100%" height="39">
				<s:fill>
					<s:BitmapFill fillMode="repeat"
								  source="@Embed('/edu/amrita/aview/core/entry/assets/images/classRoomIconBg.jpg')"/>
				</s:fill>
			</s:Rect>
			<mx:HBox id="videooptions" left="0" right="0" height="39" backgroundColor="#E7E8ED"
					 creationPolicy="all" horizontalAlign="center" horizontalGap="2"
					 verticalAlign="middle">
				<!--For Guest login: Removed video start option-->
				<s:Button id="btnStart" width="81" height="30" buttonMode="true"  enabled="{ClassroomContext.userVO.role!=Constants.MONITOR_TYPE}"
