								   backgroundImage="{activeTab}">
					
					<s:Label left="0" top="0" width="100%" height="100%"
							 color="#FFFFFF" id="userTabTxt" toolTip="Users participating in the current session"
							 fontSize="12" fontWeight="bold" text="Users" textAlign="center"
							 verticalAlign="middle"/>
				</s:BorderContainer>
				<s:Image x="65" y="0" smooth="true" smoothingQuality="high"
						 source="@Embed('/edu/amrita/aview/core/entry/assets/images/tabseprator.png')"/>
				<s:BorderContainer x="66" y="0" width="59" height="27" id="chatTab" borderVisible="false"
								   useHandCursor="true" buttonMode="true"
								   click="{leftPanelTabNav.selectedIndex=1;getBackground(leftPanelTabNav.selectedIndex)}"
								   backgroundImage="{inactiveTab}"
								   enabled="{ClassroomContext.userVO.role!=Constants.MONITOR_TYPE?true:false}">
					
					<s:Label left="0" top="0" width="100%" height="100%"
							 color="#727273" id="chatTabTxt" toolTip="Chat with all users"
							 fontSize="12" fontWeight="bold" text="Chat" textAlign="center"
							 verticalAlign="middle"/>
				</s:BorderContainer>
				<s:Image x="124" y="0" smooth="true" smoothingQuality="high"
						 source="@Embed('/edu/amrita/aview/core/entry/assets/images/tabseprator.png')"/>
				<s:BorderContainer x="125" y="0" width="59" height="27" id="viewerTab" borderVisible="false"
								   useHandCursor="true" buttonMode="true"
								   click="{leftPanelTabNav.selectedIndex=2;getBackground(leftPanelTabNav.selectedIndex)}"
								   backgroundImage="{inactiveTab}"
								   enabled="{((ClassroomContext.aviewClass.classType!=Constants.CLASS_TYPE_WEBINAR) &amp;&amp; (ClassroomContext.userVO.role!=Constants.MONITOR_TYPE))}">
					
					<s:Label left="0" top="0" width="100%" height="100%" id="viewerTabTxt"
							 color="#727273" toolTip="Shows video of interacting users and other users selected using view video"
							 fontSize="12" fontWeight="bold" text="Viewer" textAlign="center"
							 verticalAlign="middle"/>
				</s:BorderContainer>
				<s:Image x="183" y="0" smooth="true" smoothingQuality="high"
						 source="@Embed('/edu/amrita/aview/core/entry/assets/images/tabseprator.png')"/>
				<s:BorderContainer x="184" y="0" width="67" height="27" id="questionTab" borderVisible="false"
								   useHandCursor="true" buttonMode="true"
								   click="{leftPanelTabNav.selectedIndex=3;getBackground(leftPanelTabNav.selectedIndex)}"
								   backgroundImage="{inactiveTab}" 
								   enabled="{ClassroomContext.aviewClass.classType!=Constants.CLASS_TYPE_WEBINAR &amp;&amp; ClassroomContext.userVO.role!=Constants.MONITOR_TYPE }">
					
					<s:Label left="0" top="0" width="100%" height="100%"
							 color="#727273" id="questionTabTxt" toolTip="Live Questions"
							 fontSize="12" fontWeight="bold" text="Question" textAlign="center"
							 verticalAlign="middle"/>
				</s:BorderContainer>
			<mx:ViewStack id="leftPanelTabNav" left="0" right="-2" top="26" bottom="72"
							 backgroundColor="#e0effb" borderColor="#989EA9" borderVisible="true"
							 change="FlexGlobals.topLevelApplication.mainApp.mainContainerComp.classroomComp.onUserTabChange();tabIndexChangeHandler();"
							 
