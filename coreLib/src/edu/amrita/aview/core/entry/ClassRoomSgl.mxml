						  icon="{FlexGlobals.topLevelApplication.mainApp.mainContainerComp.classroomComp.startStopVideoToggleIcon}"
						  skinClass="edu.amrita.aview.core.shared.skins.BorderlessButtonSkin"
						  toolTip="Start your video" useHandCursor="true"
						  visible="{ClassroomContext.userVO.role != Strings.GUEST_TYPE}" includeInLayout="{ClassroomContext.userVO.role != Strings.GUEST_TYPE}"/>
				
				<mx:Label id="lblStart" visible="false" width="0" height="0" color="#000000"
						  fontSize="11" includeInLayout="false" paddingLeft="3" text="Start"
						  textAlign="center"/>
				<!--<s:Button id="btnMute" width="81" height="30"  useHandCursor="true" buttonMode="true"
				
				icon="{FlexGlobals.topLevelApplication.mainApp.mainContainerComp.classroomComp.muteIcon}"
				skinClass="edu.amrita.aview.core.shared.skins.BorderlessButtonSkin"
				toolTip="Mute your Audio"/>-->
				<components:ImageButton id="btnRefresh" width="81" height="30" buttonMode="true"
										click="FlexGlobals.topLevelApplication.mainApp.mainContainerComp.classroomComp.refreshVideo()"
										imageSkin="@Embed(source='/edu/amrita/aview/core/entry/assets/images/refreshbtn.png')"
										imageSkinDisabled="@Embed(source='/edu/amrita/aview/core/entry/assets/images/refreshBtndisabled.png')"
										imageSkinDown="@Embed(source='/edu/amrita/aview/core/entry/assets/images/refresh_click.png')"
										skinClass="edu.amrita.aview.core.shared.skins.ImageButtonSkin"
										toolTip="Refresh incoming video" useHandCursor="true"/>
				<!--For Web: Since there is no record feature, we don't make the Record button visible-->
				<!--Fix for issue #17108-->
				<s:Button id="btnRecord"
						  visible="{ClassroomContext.userVO.userName == ClassroomContext.moderatorName &amp;&amp; AVCEnvironment.runTime != AVCEnvironment.BROWSER}"
						  width="81" height="30" buttonMode="true"
						  click="FlexGlobals.topLevelApplication.mainApp.mainContainerComp.classroomComp.startRecord()"
						  doubleClickEnabled="false" enabled="false"
						  icon="{FlexGlobals.topLevelApplication.mainApp.mainContainerComp.classroomComp.recordIcon}"
						  includeInLayout="{ClassroomContext.userVO.userName == ClassroomContext.moderatorName &amp;&amp; AVCEnvironment.runTime != AVCEnvironment.BROWSER}"
						  mouseChildren="false"
						  skinClass="edu.amrita.aview.core.shared.skins.BorderlessButtonSkin"
						  toolTip="Record Session" useHandCursor="true"/>
				
				<!--<mx:Label id="record_label"
				visible = "false"
				width="0"
				height="0"
				text="Start"
				textAlign="center"
				includeInLayout="false"
				color="#000000"
				fontSize="11" />-->
				<mx:Label id="record_labelRecordingPart" visible="false" width="0" height="0"
						  color="#000000" fontSize="11" includeInLayout="false" paddingLeft="0"
						  text="Recording" textAlign="center"/>
				
			</mx:HBox>
		</s:BorderContainer>
		<!--<MultipleViewerInteraction:VideoStreamDisplay id="pnlTeacher" visible="false"
													  
													  includeInLayout="false" 
													  showCloseButton="true" title="Presenter"/>-->
		    <mx:Canvas id="leftPanelTabCanvas" width="100%" height="100%" borderVisible="false"
				   creationPolicy="all"
				   initialize="FlexGlobals.topLevelApplication.mainApp.mainContainerComp.classroomComp.initCamList()"
