						   verticalScrollPolicy="off" toolTip="Users participating in the current session">
				</mx:Canvas>
				<mx:Canvas id="Chat_canvas0" width="100%" height="100%" label="Chat" toolTip="Chat with all users"
						   creationPolicy="all">
				</mx:Canvas>
				<mx:Canvas id="viewerCanvas" width="100%" height="100%" label="Viewer"
						   borderVisible="true" creationPolicy="all" borderColor="#000000" borderStyle="solid"
						   focusColor="#2F8BBB" horizontalCenter="0" horizontalScrollPolicy="off"
						   verticalScrollPolicy="off" toolTip="Shows video of interacting users and other users selected using view video">
					<mx:HBox id="titleHBox" x="0" y="2" width="100%" height="20"
							 verticalAlign="middle">
						<mx:Canvas width="85%">
							<s:Label id="lblSelectedViewers" fontWeight="bold" horizontalCenter="0"
									 text="No Selected Viewer" verticalCenter="0"/>


						</mx:Canvas>

						<s:Button id="btnShowVideoWall" visible="false" width="20" height="20"
								  buttonMode="true"
								  click="FlexGlobals.topLevelApplication.mainApp.mainContainerComp.classroomComp.toggleVideoWall()"
								  icon="{FlexGlobals.topLevelApplication.mainApp.mainContainerComp.classroomComp.imgMultiViewerMaximize}"
								  skinClass="edu.amrita.aview.core.shared.skins.BorderlessButtonSkin"
								  toolTip="Show in Video Wall" useHandCursor="true"/>
					</mx:HBox>
					<mx:HRule y="{titleHBox.y+titleHBox.height + 1}" width="100%"
							  strokeColor="#000000"/>
					<MultipleViewerInteraction:viewerVideoTile id="viewersTile" x="0"
															   y="{titleHBox.y+titleHBox.height + 5}"
															   width="243" height="100%"
															   horizontalAlign="center"
															   horizontalCenter="0"
															   horizontalScrollPolicy="off"
															   tileWidth="{viewersTile.width}"/>
					
					<mx:HRule id="hruleViewVideo" visible="false"
							  y="{viewersTile.y+viewersTile.height+5}" width="100%"
							  includeInLayout="false" strokeColor="#999999"/>



					<s:Scroller y="{hruleViewVideo.y + 5}" left="50" width="100%" height="100%">

						<s:TileGroup id="viewVideoTile" visible="false" y="{hruleViewVideo.y + 5}"
									 width="180" height="100%" horizontalAlign="center"
									 horizontalCenter="0" horizontalGap="0" includeInLayout="false"
									 verticalAlign="top" verticalCenter="0" verticalGap="0">

						</s:TileGroup>

					</s:Scroller>




					<mx:ControlBar x="0" y="438" width="100%" height="100%">
						<mx:Button id="btnViewStudent" visible="false" label="ViewStudent"
								   click="FlexGlobals.topLevelApplication.mainApp.mainContainerComp.classroomComp.callViewStudent()"/>
						<mx:Button id="btnClose" visible="false" label="CloseTile"
								   click="FlexGlobals.topLevelApplication.mainApp.mainContainerComp.classroomComp.closeViewStudent()"/>
					</mx:ControlBar>
				</mx:Canvas>
				<mx:Canvas id="questionInterface" width="100%" height="100%" label="Question"
						   borderVisible="false" creationPolicy="all"
						   horizontalScrollPolicy="off" verticalScrollPolicy="off" toolTip="Live Questions">
				</mx:Canvas>
			</mx:ViewStack>

			<mx:HBox id="classRmActions" bottom="2" width="{leftPanelTabCanvas.width}"
					 height="35" horizontalAlign="center" horizontalGap="7"
					 verticalAlign="{FlexGlobals.topLevelApplication.mainApp.mainContainerComp.classroomComp.classroomContextObj.userRole == Constants.PRESENTER_ROLE || ClassroomContext.userVO.userName == ClassroomContext.moderatorName?'bottom':'middle'}">

			</mx:HBox>
		</mx:Canvas>

		<s:Spacer id="spacerBelowPresenterVideo" visible="{pnlTeacher.visible}" width="100%"
				  height="0" includeInLayout="{pnlTeacher.visible}"/>
	</mx:VBox>
		
	<!--adding a function 'resizeFunction()' for resize event-->
