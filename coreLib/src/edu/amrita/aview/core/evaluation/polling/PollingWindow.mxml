<mx:Window xmlns:mx="http://www.adobe.com/2006/mxml"
		   creationComplete="creationCompleteHandler()"
		   resize="resizeWindow()"
		   minHeight="680"
		   minWidth="920"
		   backgroundColor="#e0effb"
		   verticalScrollPolicy="off"
		   horizontalCenter="0"
		   verticalCenter="0"
		   type="normal"
		   close="closePollingWindow()"
		   layout="absolute"
		   statusBarBackgroundColor="#2a5da4"
		   status="{FlexGlobals.topLevelApplication.mainApp.COPY_RIGHT_FOOTER}">
	<mx:Script source="PollingWindowUIHandler.as"/>

	<mx:HBox id="hBoxPollingCanvas"
			 x="0"
			 y="0"
			 width="100%"
			 height="100%"
			 top="0"
			 paddingLeft="25"/>
</mx:Window>
